{
  "ci": {
    "collect": {
      "numberOfRuns": 3,
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", {"minScore": 0.95}],
        "categories:accessibility": ["warn", {"minScore": 0.90}]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
